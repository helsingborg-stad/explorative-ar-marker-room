<!doctype HTML>
html
    head
        script(src="https://aframe.io/releases/0.9.2/aframe.min.js")
        script(src="https://cdn.rawgit.com/jeromeetienne/AR.js/1.7.7/aframe/build/aframe-ar.js")
        script(src="https://raw.githack.com/donmccurdy/aframe-extras/master/dist/aframe-extras.loaders.min.js")

    body(style='margin : 0px; overflow: hidden;')
        a-scene(embedded arjs='sourceType: webcam; debugUIEnabled: false; detectionMode: mono_and_matrix; matrixCodeType: 3x3;')
        
            a-marker(type="barcode" value="0")
                a-box(position='0 0.5 0' material='color: red;')
            a-marker(type="barcode" value="1")
                a-box(position='0 0.5 0' material='color: blue;')
            a-marker(type="barcode" value="2")
                a-box(position='0 0.5 0' material='color: green;')
            a-marker(type="barcode" value="3")
                a-box(position='0 0.5 0' material='color: yellow;')
            a-marker(type="barcode" value="4")
                a-box(position='0 0.5 0' material='color: pink;')

            a-entity(camera)
